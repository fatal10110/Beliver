{
  "name": "sons-of-abrim",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:compiler": "node ./apps/compiler/src/index.js",
    "dev:simulation": "go run ./apps/simulation",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "visual:check": "node scripts/visual-check.mjs"
  },
  "devDependencies": {
    "playwright": "^1.45.0",
    "turbo": "latest",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "npm@10.0.0"
}
